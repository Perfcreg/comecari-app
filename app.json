{
  "name": "Comecari",
  "displayName": "Comecari",
  "expo": {
    "name": "Comecari",
    "slug": "comecari",
    "scheme": "comecari",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/comecari/logo.png",
    "splash": {
      "image": "./assets/images/comecari/logo.png",
